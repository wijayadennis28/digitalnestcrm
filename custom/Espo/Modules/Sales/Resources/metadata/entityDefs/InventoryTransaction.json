        "productIdType": {
            "columns": [
                "productId",
                "type"
            ]
        },
        "productIdParentId": {
            "columns": [
                "productId",
                "parentId"
            ]
        },
        "productIdWarehouseId": {
            "columns": [
                "productId",
                "warehouseId"
            ]
        },
        "productIdInventoryNumberId": {
            "columns": [
