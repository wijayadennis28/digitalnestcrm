        },
        "startTime": {
            "type": "varchar",
            "maxLength": 10,
            "options": [],
            "isCustom": true
        },
        "endTime": {
            "type": "varchar",
            "maxLength": 10,
            "options": [],
            "isCustom": true
        },
        "shift": {
            "type": "enum",
            "required": true,
            "options": [
                "1",
                "2",
                "3"
            ],
            "style": {
                "1": null,
                "2": null,
                "3": null
            },
            "default": "1",
            "maxLength": 100,
            "isCustom": true
        },
        "cProductionLogs": {
            "type": "linkMultiple",
            "layoutDetailDisabled": false,
            "layoutMassUpdateDisabled": false,
            "layoutListDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "exportDisabled": false,
            "customizationDisabled": false,
            "isCustom": true
        },
        "users": {
            "type": "linkMultiple",
            "layoutDetailDisabled": false,
            "layoutMassUpdateDisabled": false,
            "layoutListDisabled": false,
            "noLoad": false,
            "importDisabled": false,
            "exportDisabled": false,
            "customizationDisabled": false,
            "isCustom": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "cProductionLogs": {
            "type": "hasMany",
            "foreign": "cWorkingShift",
            "entity": "CProductionLog",
            "audited": true,
            "isCustom": true
        },
        "users": {
            "type": "hasMany",
            "relationName": "cUserCWorkingShift",
            "foreign": "cCWorkingShifts",
            "entity": "User",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}