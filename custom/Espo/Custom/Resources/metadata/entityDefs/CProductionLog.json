{
    "fields": {
        "name": {
            "type": "varchar",
            "required": false,
            "pattern": "$noBadCharacters",
            "options": [],
            "readOnly": true
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "notNull": false,
            "type": "datetime",
            "readOnly": true,
            "default": null,
            "minuteStep": 30,
            "required": false
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": true,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "shift": {
            "type": "enum",
            "required": true,
            "options": [
                "1",
                "2",
                "3"
            ],
            "style": {
                "1": null,
                "2": null,
                "3": null
            },
            "default": "1",
            "maxLength": 100,
            "isCustom": true
        },
        "batchCode": {
            "type": "varchar",
            "maxLength": 100,
            "options": [],
            "readOnly": true,
            "isCustom": true
        },
        "batch": {
            "type": "varchar",
            "required": true,
            "maxLength": 100,
            "options": [],
            "isCustom": true
        },
        "machine": {
            "type": "varchar",
            "required": true,
            "maxLength": 100,
            "options": [
                "Machine A",
                "Machine B"
            ],
            "isCustom": true
        },
        "productionStatus": {
            "type": "enum",
            "options": [
                "Failed",
                "Partial",
                "Successful"
            ],
            "style": {
                "Failed": "danger",
                "Partial": "processed",
                "Successful": "success"
            },
            "default": "Failed",
            "maxLength": 100,
            "isCustom": true
        },
        "reasonOfFailure": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "actionTaken": {
            "type": "text",
            "rowsMin": 2,
            "cutHeight": 200,
            "isCustom": true
        },
        "cIngredientsAmounts": {
            "type": "linkMultiple",
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true,
            "layoutListDisabled": true,
            "noLoad": true,
            "importDisabled": true,
            "exportDisabled": true,
            "customizationDisabled": true,
            "isCustom": true
        },
        "warehouse": {
            "type": "link"
        },
        "productionLogNumber": {
            "type": "number",
            "len": 36,
            "notNull": false,
            "unique": false,
            "nextNumber": 1,
            "padLength": 4,
            "prefix": "PL0000-",
            "isCustom": true
        },
        "cWorkingShift": {
            "type": "link"
        },
        "cProductionLogItems": {
            "type": "linkMultiple",
            "layoutDetailDisabled": true,
            "layoutMassUpdateDisabled": true,
            "layoutListDisabled": true,
            "noLoad": true,
            "importDisabled": true,
            "exportDisabled": true,
            "customizationDisabled": true,
            "isCustom": true
        },
        "startProductionTime": {
            "notNull": false,
            "type": "datetime",
            "useNumericFormat": true,
            "minuteStep": 1,
            "isCustom": true,
            "required": true,
            "default": null,
            "audited": true
        },
        "endProductionTime": {
            "notNull": false,
            "type": "datetime",
            "minuteStep": 1,
            "isCustom": true,
            "required": true,
            "useNumericFormat": true,
            "audited": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "cIngredientsAmounts": {
            "type": "hasMany",
            "foreign": "cProductionLog",
            "entity": "CIngredientsAmount",
            "audited": false,
            "isCustom": true
        },
        "warehouse": {
            "type": "belongsTo",
            "foreign": "cCProductionLogs",
            "entity": "Warehouse",
            "audited": false,
            "isCustom": true
        },
        "cWorkingShift": {
            "type": "belongsTo",
            "foreign": "cProductionLogs",
            "entity": "CWorkingShift",
            "audited": true,
            "isCustom": true
        },
        "cProductionLogItems": {
            "type": "hasMany",
            "foreign": "cProductionLog",
            "entity": "CProductionLogItem",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc",
        "textFilterFields": [
            "name"
        ],
        "fullTextSearch": false,
        "countDisabled": false
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "optimisticConcurrencyControl": false
}