{
    "fields": {
        "name": {
            "type": "varchar",
            "required": false,
            "pattern": "$noBadCharacters",
            "options": []
        },
        "description": {
            "type": "text"
        },
        "createdAt": {
            "type": "datetime",
            "readOnly": true
        },
        "modifiedAt": {
            "type": "datetime",
            "readOnly": true
        },
        "createdBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "modifiedBy": {
            "type": "link",
            "readOnly": true,
            "view": "views/fields/user"
        },
        "assignedUser": {
            "type": "link",
            "required": false,
            "view": "views/fields/assigned-user"
        },
        "teams": {
            "type": "linkMultiple",
            "view": "views/fields/teams"
        },
        "cProductionLog": {
            "type": "link"
        },
        "qtyProduced": {
            "type": "int",
            "isCustom": true,
            "required": true,
            "min": 0,
            "default": 0
        },
        "qtyFailed": {
            "type": "int",
            "isCustom": true,
            "required": true,
            "default": 0,
            "min": 0
        },
        "product": {
            "type": "link",
            "required": true
        },
        "inventoryNumber": {
            "type": "link",
            "required": true
        },
        "cProductFormula": {
            "type": "link",
            "required": true
        }
    },
    "links": {
        "createdBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "modifiedBy": {
            "type": "belongsTo",
            "entity": "User"
        },
        "assignedUser": {
            "type": "belongsTo",
            "entity": "User"
        },
        "teams": {
            "type": "hasMany",
            "entity": "Team",
            "relationName": "entityTeam",
            "layoutRelationshipsDisabled": true
        },
        "cProductionLog": {
            "type": "belongsTo",
            "foreign": "cProductionLogItems",
            "entity": "CProductionLog",
            "audited": false,
            "isCustom": true
        },
        "product": {
            "type": "belongsTo",
            "foreign": "cCProductionLogItems",
            "entity": "Product",
            "audited": false,
            "isCustom": true
        },
        "inventoryNumber": {
            "type": "belongsTo",
            "foreign": "cCProductionLogItems",
            "entity": "InventoryNumber",
            "audited": false,
            "isCustom": true
        },
        "cProductFormula": {
            "type": "belongsTo",
            "foreign": "cProductionLogItems",
            "entity": "CProductFormula",
            "audited": false,
            "isCustom": true
        }
    },
    "collection": {
        "orderBy": "createdAt",
        "order": "desc"
    },
    "indexes": {
        "name": {
            "columns": [
                "name",
                "deleted"
            ]
        },
        "assignedUser": {
            "columns": [
                "assignedUserId",
                "deleted"
            ]
        },
        "createdAt": {
            "columns": [
                "createdAt"
            ]
        },
        "createdAtId": {
            "unique": true,
            "columns": [
                "createdAt",
                "id"
            ]
        }
    },
    "hooks": {
        "afterRemove": [
            "Espo\\Custom\\Hooks\\CProductionLogItem\\AfterRemove"
        ]
    }
}